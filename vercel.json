

  {
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "api/users.js"
      },
      {
        "src": "api/products",
        "dest": "api/products.js"
      },
      {
        "src": "/api/login",
        "dest": "api/login.js"
      },
      {
        "src": "/api/adminregistration",
        "dest": "api/adminregistration.js"
      },
      {
        "src": "/api/adminlogin",
        "dest": "api/adminlogin.js"
      },
      {
        "src": "/(.*)",
        "dest": "api/adminregistration.js"
      }
      
    ]
  }