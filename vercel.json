

  {
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "api/users.js"
      },
      {
        "src": "/api/products",
        "dest": "api/products.js"
      },
      {
        "src": "/api",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ]
  }