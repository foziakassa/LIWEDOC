{
  "version": 2,
  "builds": [
    
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "api/users.js"
    },
    {
      "src": "/api/products",
      "dest": "api/products.js"
    },
    {
      "src": "/api/login",
      "dest": "api/login.js"
    },
    {
      "src": "/api/adminregistration",
      "dest": "api/adminregistration.js"
    },
    {
      "src": "/api/adminlogin",
      "dest": "api/adminlogin.js"
    },
   
    
    {
      "src": "/api/swap-requests/(.*)",
      "dest": "/api/swap-requests.js"
    },
    {
      "src": "/api/notifications/(.*)",
      "dest": "/api/notifications.js"
    },
    {
      "src": "/api/messages/(.*)",
      "dest": "/api/messages.js"
    },
    {
      "src": "/api/search/(.*)",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/api/analytics.js"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/homepage",
      "dest": "/server.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/dashboard",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    },
    {
      "src": "/api/plans",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/payment/initialize",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/payment/verify/(.*)",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/payment/callback",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/user/posts-status",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/user/create-post",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/payment/receipt/(.*)",
      "dest": "api/payment.js"
    },
    {
      "src": "/health",
      "dest": "api/payment.js"
    },
    {
      "src": "/api/transactions",
      "dest": "api/payment.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/userregistration.js"
    }
  ]
 
}
